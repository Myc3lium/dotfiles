## URxvt settings
## define FONT "System San Francisco Display"
#define FONT "FreeMono Regular"

! text formatting
URxvt.letterSpace: -5
URxvt.font: xft: FONT: size=10: Regular
URxvt.boldFont: xft: FONT: size=10: Bold
URxvt.italicFont: xft: FONT: size=10: Italic
URxvt.boldItalicFont: xft: FONT: size=13: Bold: Italic

! extensions
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

! Xterm escapes, word by word movement
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: full

URxvt.scrollBar: false
URxvt.geometry: 81x23

## include ".xfiles/apprentice"
## include ".cache/wal/colors.Xresources"

## i3wm.foreground:
## i3wm.background: 

## XTerm settings
XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.vt100.saveLines: 4096
XTerm.vt100.faceName: System San Francisco Display:size=10:antialias=false
